<style type="text/css">
#warap {
width:<%=@width%>px;
height:<%=@height%>px;
}

#left {
	position:relative;
	float:left;
	margin:0 auto;
	width:50%;
	height:100%;
}

#righttop,#rightbottom {
	position:relative;
	float:left;
	margin:0 auto;
	width:50%;
	height:50%;
}

#left img {
  position:absolute;
  left:0;
  width:100%;
  height:100%;
}
#righttop img {
  position:absolute;
  left:0;
  width:100%;
  height:100%;
}
#rightbottom img {
  position:absolute;
  left:0;
  width:100%;
  height:100%;
}
#infoi {
  position:absolute;
  left:0;

  width:100%;
  height:10%;

  font-size: 100px;
  background-color:black;
  opacity:0.6;
  z-index: 0;
  color: black;
}
#infoi p{
  vertical-align:bottom;
  margin:30px 40px;
  font-weight:bold;
  font-size: 10%;
  color : white;
}

  @-webkit-keyframes cf3FadeInOut {
   0% {
     opacity:1;
   }
   25% {
    opacity:1;
  }
  75% {
    opacity:0;
  }
  100% {
   opacity:0;
 }
}

@-moz-keyframes cf3FadeInOut {
 0% {
   opacity:1;
 }
 25% {
  opacity:1;
}
75% {
  opacity:0;
}
100% {
 opacity:0;
}
}

@-o-keyframes cf3FadeInOut {
 0% {
   opacity:1;
 }
